{
  "name": "rdi",
  "version": "1.0.1",
  "description": "A port of Angular's ReflectiveInjector",
  "main": "lib/index.js",
  "typings": "lib/index.d.js",
  "scripts": {
    "prepublish": "npm run build",
    "test": "jest",
    "test:watch": "jest --watch",
    "build": "tsc -p tsconfig.build.json"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ItsDizzy/rdi.git"
  },
  "keywords": [
    "di",
    "typescript",
    "angular",
    "dependency",
    "injector",
    "decorators",
    "annotations",
    "reflect",
    "metadata"
  ],
  "author": "Dizzy <itsdizzy40@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ItsDizzy/rdi/issues"
  },
  "homepage": "https://github.com/ItsDizzy/rdi#readme",
  "dependencies": {
    "reflect-metadata": "^0.1.12"
  },
  "devDependencies": {
    "@types/jest": "^22.1.2",
    "@types/node": "^9.4.6",
    "jest": "^22.3.0",
    "ts-jest": "^22.0.4",
    "typescript": "^2.7.2"
  },
  "jest": {
    "modulePathIgnorePatterns": [
      "packages/.*/lib"
    ],
    "transform": {
      ".(ts|tsx)": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/packages/.*/lib"
    ],
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json"
    ]
  }
}
